@charset "UTF-8";
html,body,div,h1,h2,h3,h4,h5,h6,p,img,dl,dt,dd,ol,ul,li,table,caption,tbody,tfoot,thead,tr,th,td,form,fieldset,embed,object,applet{margin:0;padding:0;border:0}
body{color:#000;background:#fff;font-size:1em;font-family:"Helvetica Neue", Arial, Helvetica, sans-serif;min-width:1200px}
#body-wrapper{position:relative;width:1280px}
.form-wrapper,.module-container{padding:14px}
.form-style,.module-container{border:solid 2px #ccc;-moz-border-radius:5px; -webkit-border-radius:5px; border-radius:5px;}
.form-style h1,.module-container h1{font-size:1em;font-weight:bold;margin-bottom:8px}
.form-style p.description{font-size:0.7em;color:#666;margin-bottom:20px;border-bottom:solid 1px #ccc;padding-bottom:10px}
.form-style p{font-size:0.6em}
.form-style label{display:block;font-weight:bold;text-align:right;width:42%;float:left}
.form-style p.label{font-size:1em}
*+html .form-style p.label{width:auto;text-align:right;width:29%;margin-left:70%} /* IE-7 only hack */
.form-style .small{color:#666;display:block;font-size:0.6em;font-weight:normal;text-align:right;width:100%}
.form-style input,.form-style textarea,.form-style select{float:left;font-size:1em;padding:4px 2px;border:solid 1px #dde5f9;width:50%;margin:2px 0 10px 10px}
.form-style textarea{width:95%}
.form-style button{margin-top: 10px;margin-left:42%}
.form-style .requiredPrefix,.form-style .error{color:red}
.form-style .error{float:left;margin-left:42%;margin-bottom:10px}
.form-style table {font:inherit;font-family:inherit;border-collapse:collapse;empty-cells:show;border:0;margin:0;padding:0}
.form-style table th {padding:10px 50px 10px 10px}
.address,.elevation,.levee,.waterDepth {font-size:0.9em}
.even{background-color:#f6f6f6}
#results{display:none}
#results,#results table{margin-left:auto;margin-right:auto;width:99%}
#results table tbody :hover{background-color:#ddd}
#results th {padding:0;margin:0;text-align:left}
.ui-icon-loading {background: url(../images/loading_animated.gif) no-repeat center right;padding-right:25px;padding-bottom:15px}
#footer{margin-top:10px;color:#777;font-size:0.65em;width:1100px;text-align:center}
#footer-wrapper{margin-top:10px;color:#777;font-size:0.65em;width:728px;text-align:center;width:100%}
.heading{height:1.6em;padding:.3em .5em 0 .5em;white-space:nowrap;font-size:0.5em}
#content{width:59%}
#side-content{float:right;width:41%}
#content-wrapper{margin-left:1em;margin-top:1em;width:97%}
.clear{clear:both}
.small-info {color:#777;font-size:0.65em}
ul{padding:0 0 1em 1em;list-style:none}
li{line-height:1.5em;padding:0 0 .5em 0;font-size:0.8em}
.module-container h1 {margin-left:auto;margin-right:auto;width:100%;text-align:center;padding-bottom:10px}
#more-options{display:none}